{
  "name": "docker-librato",
  "version": "1.1.5",
  "description": "Forward all stats from all running docker containers to Librato",
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/docker-librato.git"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-"
  },
  "author": "Meteorhacks",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.3.3",
    "docker-allcontainers": "^0.6.1",
    "docker-stats": "^0.7.0",
    "envalid": "^2.4.1",
    "librato-node": "^4.1.0",
    "lodash": "^4.17.2",
    "sigterm-handler": "^2.0.0",
    "through2": "2.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-subset": "^1.3.0",
    "mocha": "^3.1.2",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^9.0.1",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
